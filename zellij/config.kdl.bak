default_layout "compact"
keybinds clear-defaults=true {
        entersearch {
                bind "Ctrl c" "Esc" {
                        SwitchToMode "Scroll"
                }
                bind "Enter" {
                        SwitchToMode "Search"
                }
        }
        locked {
                bind "Alt z" {
                        MessagePlugin {
                                autolock {
                                        payload "disable"
                                }
                        }
                        SwitchToMode "Normal"
                }
                bind "Ctrl g" {
                        SwitchToMode "Normal"
                }
        }
        move {
                bind "Ctrl m" {
                        SwitchToMode "Normal"
                }
                bind "h" {
                        MovePane "Left"
                }
                bind "j" {
                        MovePane "Down"
                }
                bind "k" {
                        MovePane "Up"
                }
                bind "l" {
                        MovePane "Right"
                }
                bind "n" "Tab" {
                        MovePane
                }
                bind "p" {
                        MovePaneBackwards
                }
        }
        normal {
                bind "Ctrl n" {
                        LaunchOrFocusPlugin "session-manager" {
                                floating true
                                move_to_focused_tab true
                        }
                        SwitchToMode "Normal"
                }
                bind "Enter" {
                        MessagePlugin "autolock" {

                        }
                        WriteChars "\u{000D}"
                }
        }
        pane {
                bind "Ctrl w" {
                        SwitchToMode "Normal"
                }
                bind "c" {
                        PaneNameInput 0
                        SwitchToMode "RenamePane"
                }
                bind "d" {
                        NewPane "Down"
                        SwitchToMode "Normal"
                }
                bind "e" {
                        SwitchToMode "Normal"
                        TogglePaneEmbedOrFloating
                }
                bind "f" {
                        SwitchToMode "Normal"
                        ToggleFocusFullscreen
                }
                bind "h" {
                        MoveFocus "Left"
                }
                bind "j" {
                        MoveFocus "Down"
                }
                bind "k" {
                        MoveFocus "Up"
                }
                bind "l" {
                        MoveFocus "Right"
                }
                bind "n" {
                        NewPane
                        SwitchToMode "Normal"
                }
                bind "p" {
                        SwitchFocus
                }
                bind "r" {
                        NewPane "Right"
                        SwitchToMode "Normal"
                }
                bind "w" {
                        SwitchToMode "Normal"
                        ToggleFloatingPanes
                }
                bind "x" {
                        CloseFocus
                        SwitchToMode "Normal"
                }
                bind "z" {
                        SwitchToMode "Normal"
                        TogglePaneFrames
                }
        }
        renamepane {
                bind "Ctrl c" {
                        SwitchToMode "Normal"
                }
                bind "Esc" {
                        SwitchToMode "Pane"
                        UndoRenamePane
                }
        }
        renametab {
                bind "Ctrl c" {
                        SwitchToMode "Normal"
                }
                bind "Esc" {
                        SwitchToMode "Tab"
                        UndoRenameTab
                }
        }
        resize {
                bind "-" {
                        Resize "Decrease"
                }
                bind "=" "+" {
                        Resize "Increase"
                }
                bind "H" {
                        Resize "Decrease Left"
                }
                bind "J" {
                        Resize "Decrease Down"
                }
                bind "K" {
                        Resize "Decrease Up"
                }
                bind "L" {
                        Resize "Decrease Right"
                }
                bind "h" {
                        Resize "Increase Left"
                }
                bind "j" {
                        Resize "Increase Down"
                }
                bind "k" {
                        Resize "Increase Up"
                }
                bind "l" {
                        Resize "Increase Right"
                }
        }
        scroll {
                bind "Ctrl b" "h" {
                        PageScrollUp
                }
                bind "Ctrl c" {
                        ScrollToBottom
                        SwitchToMode "Normal"
                }
                bind "Ctrl f" "l" {
                        PageScrollDown
                }
                bind "Ctrl s" {
                        SwitchToMode "Normal"
                }
                bind "d" {
                        HalfPageScrollDown
                }
                bind "e" {
                        EditScrollback
                        SwitchToMode "Normal"
                }
                bind "j" {
                        ScrollDown
                }
                bind "k" {
                        ScrollUp
                }
                bind "s" {
                        SearchInput 0
                        SwitchToMode "EnterSearch"
                }
                bind "u" {
                        HalfPageScrollUp
                }
        }
        search {
                bind "Ctrl b" "h" {
                        PageScrollUp
                }
                bind "Ctrl c" {
                        ScrollToBottom
                        SwitchToMode "Normal"
                }
                bind "Ctrl f" "l" {
                        PageScrollDown
                }
                bind "Ctrl s" {
                        SwitchToMode "Normal"
                }
                bind "c" {
                        SearchToggleOption "CaseSensitivity"
                }
                bind "d" {
                        HalfPageScrollDown
                }
                bind "j" {
                        ScrollDown
                }
                bind "k" {
                        ScrollUp
                }
                bind "n" {
                        Search "down"
                }
                bind "o" {
                        SearchToggleOption "WholeWord"
                }
                bind "p" {
                        Search "up"
                }
                bind "u" {
                        HalfPageScrollUp
                }
                bind "w" {
                        SearchToggleOption "Wrap"
                }
        }
        session {
                bind "Ctrl o" {
                        SwitchToMode "Normal"
                }
                bind "Ctrl s" {
                        SwitchToMode "Scroll"
                }
                bind "c" {
                        LaunchOrFocusPlugin "configuration" {
                                floating true
                                move_to_focused_tab true
                        }
                        SwitchToMode "Normal"
                }
                bind "d" {
                        Detach
                }
                bind "w" {
                        LaunchOrFocusPlugin "session-manager" {
                                floating true
                                move_to_focused_tab true
                        }
                        SwitchToMode "Normal"
                }
        }
        shared {
                bind "Alt Shift z" {
                        MessagePlugin {
                                autolock {
                                        payload "enable"
                                }
                        }
                }
        }
        shared_except "locked" {
                bind "Alt -" {
                        Resize "Decrease"
                }
                bind "Alt =" "Alt +" {
                        Resize "Increase"
                }
                bind "Alt [" {
                        PreviousSwapLayout
                }
                bind "Alt ]" {
                        NextSwapLayout
                }
                bind "Alt f" {
                        ToggleFloatingPanes
                }
                bind "Alt h" "Alt Left" {
                        MoveFocusOrTab "Left"
                }
                bind "Alt i" {
                        MoveTab "Left"
                }
                bind "Alt j" "Alt Down" {
                        MoveFocus "Down"
                }
                bind "Alt k" "Alt Up" {
                        MoveFocus "Up"
                }
                bind "Alt l" "Alt Right" {
                        MoveFocusOrTab "Right"
                }
                bind "Alt n" {
                        NewPane
                }
                bind "Alt o" {
                        MoveTab "Right"
                }
                bind "Alt z" {
                        MessagePlugin {
                                autolock {
                                        payload "disable"
                                }
                        }
                        SwitchToMode "Normal"
                }
                bind "Ctrl g" {
                        SwitchToMode "Locked"
                }
        }
        shared_except "move" "locked" {
                bind "Ctrl m" {
                        SwitchToMode "Move"
                }
        }
        shared_except "normal" "locked" {
                bind "Enter" "Esc" "q" {
                        SwitchToMode "Normal"
                }
        }
        shared_except "pane" "locked" {
                bind "Ctrl w" {
                        SwitchToMode "Pane"
                }
        }
        shared_except "scroll" "locked" {
                bind "Ctrl s" {
                        SwitchToMode "Scroll"
                }
        }
        shared_except "session" "locked" {
                bind "Ctrl o" {
                        SwitchToMode "Session"
                }
        }
        shared_except "tab" "locked" {
                bind "Ctrl t" {
                        SwitchToMode "Tab"
                }
        }
        shared_except "tmux" "locked" {
                bind "Ctrl b" {
                        SwitchToMode "Tmux"
                }
        }
        tab {
                bind "1" {
                        GoToTab 1
                        SwitchToMode "Normal"
                }
                bind "2" {
                        GoToTab 2
                        SwitchToMode "Normal"
                }
                bind "3" {
                        GoToTab 3
                        SwitchToMode "Normal"
                }
                bind "4" {
                        GoToTab 4
                        SwitchToMode "Normal"
                }
                bind "5" {
                        GoToTab 5
                        SwitchToMode "Normal"
                }
                bind "6" {
                        GoToTab 6
                        SwitchToMode "Normal"
                }
                bind "7" {
                        GoToTab 7
                        SwitchToMode "Normal"
                }
                bind "8" {
                        GoToTab 8
                        SwitchToMode "Normal"
                }
                bind "9" {
                        GoToTab 9
                        SwitchToMode "Normal"
                }
                bind "Ctrl t" {
                        SwitchToMode "Normal"
                }
                bind "Tab" {
                        ToggleTab
                }
                bind "[" {
                        BreakPaneLeft
                }
                bind "]" {
                        BreakPaneRight
                }
                bind "b" {
                        BreakPane
                        SwitchToMode "Normal"
                }
                bind "h" "k" {
                        GoToPreviousTab
                }
                bind "l" "j" {
                        GoToNextTab
                }
                bind "n" {
                        NewTab
                        SwitchToMode "Normal"
                }
                bind "r" {
                        SwitchToMode "RenameTab"
                        TabNameInput 0
                }
                bind "s" {
                        SwitchToMode "Normal"
                        ToggleActiveSyncTab
                }
                bind "x" {
                        CloseTab
                        SwitchToMode "Normal"
                }
        }
        tmux {
                bind "%" {
                        NewPane "Right"
                        SwitchToMode "Normal"
                }
                bind "," {
                        SwitchToMode "RenameTab"
                }
                bind "Ctrl b" {
                        SwitchToMode "Normal"
                        Write 2
                }
                bind "Space" {
                        NextSwapLayout
                }
                bind "[" {
                        SwitchToMode "Scroll"
                }
                bind "\"" {
                        NewPane "Down"
                        SwitchToMode "Normal"
                }
                bind "c" {
                        NewTab
                        SwitchToMode "Normal"
                }
                bind "d" {
                        Detach
                }
                bind "h" {
                        MoveFocus "Left"
                        SwitchToMode "Normal"
                }
                bind "j" {
                        MoveFocus "Down"
                        SwitchToMode "Normal"
                }
                bind "k" {
                        MoveFocus "Up"
                        SwitchToMode "Normal"
                }
                bind "l" {
                        MoveFocus "Right"
                        SwitchToMode "Normal"
                }
                bind "n" {
                        GoToNextTab
                        SwitchToMode "Normal"
                }
                bind "o" {
                        FocusNextPane
                }
                bind "p" {
                        GoToPreviousTab
                        SwitchToMode "Normal"
                }
                bind "x" {
                        CloseFocus
                        SwitchToMode "Normal"
                }
                bind "z" {
                        SwitchToMode "Normal"
                        ToggleFocusFullscreen
                }
        }
}
load_plugins {
        autolock
}
mirror_session false
pane_frames false
plugins {
        autolock location="file:~/.config/zellij/plugins/zellij-autolock.wasm" {
                is_enabled true
                print_to_log true
                reaction_seconds "0.5"
                triggers "nvim|vim|fzf|zoxide|atuin"
        }
}
theme "custom"
themes {
        custom {
                bg "#15161e"
                black "#15161e"
                blue "#7aa2f7"
                cyan "#7dcfff"
                fg "#a9b1d6"
                green "#9ece6a"
                magenta "#bb9af7"
                orange "#ff9e64"
                red "#f7768e"
                white "#a9b1d6"
                yellow "#e0af68"
        }
}
ui {
        pane_frames {
                rounded_corners true
        }
}
